{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "cd .. && npm install",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "includeFiles": "dist/**",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api?path=:path*"
    }
  ]
}
